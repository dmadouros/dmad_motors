<table class="cars">
  <tr>
    <th class="cars__header cars__make">Make</th>
    <th class="cars__header cars__model">Model</th>
    <th class="cars__header cars__model-year">Model Year</th>
  </tr>
  <% @cars.each do |car| %>
    <tr>
      <td class="cars__make"><%= car.make %></td>
      <td class="cars__model"><%= car.model %></td>
      <td class="cars__model-year"><%= car.model_year %></td>
    </tr>
  <% end %>
</table>